# see /usr/share/icewm/keys

key "Ctrl+Shift+Esc" xterm -name htop -title htop -maximized -e htop
key "Super+p" xterm -maximized -e sudo powertop
key "Super+b" xterm -e yacpi
key "Super+e" emelfm2
key "Super+l" xterm -e xlock
key "Super+v" xterm -e aumix
key "Super+w" xterm -e wicd-curses
key "XF86Sleep" xterm -e sudo pm-suspend
key "XF86PowerOff" xterm -e sudo pm-suspend

