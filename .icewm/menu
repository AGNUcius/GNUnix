menu System system {
 menu Power power {
  prog Lock xtrlock xtrlock
  prog Suspend suspend xterm -e sudo pm-suspend
  prog Reboot reboot xterm -e sudo shutdown -r now
  prog Shutdown shutdown xterm -e sudo shutdown -P now
  prog Refresh refresh killall -SIGHUP icewm
  prog Powertop powertop xterm -maximized -e sudo powertop
 }

 menu Programs programs {
  prog "Manage Tasks" htop xterm -maximized -e htop
  prog "Manage Tasks as root" htop xterm -maximized -e sudo htop
  prog "Kill Window" xkill xterm -e sudo xkill
  prog "App Shop" software-center software-center
  prog "Add/Remove Programs" synaptic xterm -e sudo synaptic
 }

 menu Video video {
  prog "Reset Acer Laptop" xrandr xrandr --output LVDS1 --mode 1024x600 --output VGA1 --off
  prog "1024x768 on VGA1"  xrandr xrandr --output LVDS1 --off --output VGA1 --mode 1024x768
  prog "1280x1024 on VGA1" xrandr xrandr --output LVDS1 --off --output VGA1 --mode 1280x1024
separator
  prog "Disable Screen Blanking" xset xterm -e "xset s noblank; xset -dpms; xset s off"
  prog "ReEnable Screen Blanking" xset xterm -e "xset s blank; xset +dpms; xset s on"
 }

 prog "Check Battery" yacpi xterm -e yacpi
 prog "Adjust Volume" alsamixer xterm -e alsamixer
 prog "Fix Whacky Mouse" mouse xterm -e "sudo modprobe -r psmouse; sudo modprobe psmouse"
 prog "CapsLock=>Ctrl" setxkbmap setxkbmap -option ctrl:nocaps

# separator
# prog "Make LiveCD" xterm xterm -e
# separator
 prog RootMacs emacs xterm -e sudo emacs --maximized --eval '(set-background-color "skyblue")'
}

