menu System system {
 menu Power power {
  prog Lock xlock xlock
  prog Suspend suspend xterm -e sudo pm-suspend
  prog Restart restart xterm -e sudo shutdown -r now
  prog Shutdown shutdown xterm -e sudo shutdown -P now
  prog "Power Settings" powertop xterm -maximized -e sudo powertop
 }

 menu Programs programs {
  prog "App Shop" software-center software-center
  prog "Add/Remove Programs" synaptic xterm -e sudo synaptic
 }

 prog "Manage Printers" cups chromium-browser http://localhost:631
 prog "Manage Tasks" htop xterm -maximized -e htop
 prog "Murder Window" xkill xterm -e sudo xkill
 prog "Fix Whacky Mouse" mouse xterm -e "sudo modprobe -r psmouse; sudo modprobe psmouse"
 prog "Fix Philips TV Screen" xrandr --output VGA1 --mode 1024x768
 prog "Fix Acer One Laptop Screen" screen xrandr --output  LVDS1 --mode 1024x600
 prog "Check Battery" yacpi xterm -e yacpi
 prog "Adjust Volume" alsamixer xterm -e alsamixer
# separator
# prog "Make LiveCD" xterm xterm -e
# separator
 prog RootMacs emacs xterm -e sudo emacs --maximized --eval '(set-background-color "darkgreen")'
}

