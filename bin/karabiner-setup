#!/bin/sh

cli=/Applications/Karabiner.app/Contents/Library/bin/karabiner

$cli set remap.pc_style_copy_paste 1
/bin/echo -n .
$cli set remap.app_vm_commandL2optionL 1
/bin/echo -n .
$cli set remap.pclikecontrolarrow_except_vm 1
/bin/echo -n .
$cli set remap.redo_winstyle_no_term 1
/bin/echo -n .
$cli set remap.app_google_chrome_controlW2commandW 1
/bin/echo -n .
$cli set remap.pointing_controlLeftclick2commandLeftclick 1
/bin/echo -n .
$cli set remap.find_winstyle_no_term 1
/bin/echo -n .
$cli set repeat.wait 33
/bin/echo -n .
$cli set remap.pcstyle_close 1
/bin/echo -n .
$cli set remap.new_winstyle_no_term 1
/bin/echo -n .
$cli set remap.pc_style_open 1
/bin/echo -n .
$cli set remap.pclikehomeend_emacs 1
/bin/echo -n .
$cli set remap.app_google_chrome_controlL2commandL 1
/bin/echo -n .
$cli set remap.undo_winstyle_no_term 1
/bin/echo -n .
$cli set remap.pc_application2rightclick 1
/bin/echo -n .
$cli set remap.pclikehomeend 1
/bin/echo -n .
$cli set remap.save_winstyle_no_term 1
/bin/echo -n .
$cli set remap.optionL2commandL 1
/bin/echo -n .
$cli set remap.copy_paste_winstyle_new_tab 1
/bin/echo -n .
$cli set remap.pclikepageupdown_except_emacs 1
/bin/echo -n .
$cli set remap.select_all_winstyle_no_term 1
/bin/echo -n .
$cli set repeat.initial_wait 416
/bin/echo -n .
/bin/echo
